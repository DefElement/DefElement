symfem:
  name: Symfem
  url: https://github.com/mscroggs/symfem
  install: pip3 install symfem
basix:
  name: Basix
  url: https://github.com/FEniCS/basix
  install: pip3 install git+https://github.com/FEniCS/basix.git
