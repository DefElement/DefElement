name: dual polynomial
html-name: dual polynomial
short-names:
  - dual
ndofs:
  dual polygon(n): \begin{cases}1&k=0\\n&k=1\end{cases}
min-order: 0
max-order: 1
categories:
  - scalar
reference elements:
  - dual polygon
symfem: dual
examples:
  - dual polygon(6),0
  - dual polygon(4),1
  - dual polygon(5),1
